    {
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [ "error", 4 ],
        "quotes": [ "error", "single" ],
        "semi": [ "error", "always" ],
        "no-console": 1
    }
}